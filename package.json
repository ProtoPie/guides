{
    "name": "@protopie/guides",
    "version": "1.0.1",
    "description": "A Vanilla Guides component that can draw ruler and manage guidelines.",
    "main": "./dist/guides.min.js",
    "sideEffects": false,
    "types": "declaration/index.d.ts",
    "scripts": {
        "build:prod": "npx webpack --mode production",
        "start": "npx webpack -w",
        "postinstall": "npm i webpack && npm i webpack-cli && npm run build:prod && npm run declaration",
        "lint": "npx eslint src/**/*.{ts,tsx}",
        "lint:fix": "npx eslint src/**/*.{ts,tsx} --fix",
        "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ProtoPie/guides.git"
    },
    "author": "Protopie",
    "license": "MIT",
    "devDependencies": {
        "@types/react": "^16.9.17",
        "@types/react-dom": "^16.9.4",
        "@typescript-eslint/eslint-plugin": "^5.41.0",
        "@typescript-eslint/parser": "^5.41.0",
        "eslint": "^8.26.0",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-unused-imports": "^2.0.0",
        "ts-loader": "^9.4.1",
        "tslint": "^5.18.0",
        "typescript": "4.8.4",
        "webpack": "^5.75.0",
        "webpack-cli": "^4.10.0"
    },
    "dependencies": {
        "@scena/event-emitter": "^1.0.4",
        "autobind-decorator": "^2.4.0",
        "css-to-mat": "^1.0.3",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.5.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "gesto": "^1.0.0",
        "prettier": "^2.7.1",
        "react": "^16.12.0",
        "react-css-styled": "^1.0.4",
        "react-dom": "^16.12.0"
    }
}
